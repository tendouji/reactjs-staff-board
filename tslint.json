{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "member-access": [
      true,
      "no-public"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-unused-variable": false,
    "no-console": false,
    "jsdoc-format": true
  },
  "defaultSeverity": "warning"
}
